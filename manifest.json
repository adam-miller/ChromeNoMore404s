{
  "manifest_version": 2,

  "name": "No More 404s",
  "description": "This is a test of no more 404s chrome integration",
  "version": "1.0",

  "permissions": [
    "tabs",
    "webRequest",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "web_accessible_resources": [
    "index.html"
  ],
  "browser_action": {
    "default_title": "No more 404s!",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}